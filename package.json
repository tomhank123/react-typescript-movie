  "main": "index.js",
  "scripts": {
    "admin": "npm i && webpack-dev-server --mode development --config webpack/webpack.base.config.js --open --hot --history-api-fallback --env.ENVIRONMENT=dev --env.ROLE=ua",
    "client": "npm i && webpack-dev-server --mode development --config webpack/webpack.base.config.js --open --hot --history-api-fallback --env.ENVIRONMENT=dev --env.ROLE=uc",
    "prebuild": "npm run clean",
    "build-staging-admin": "npm run clean && webpack --mode production --config webpack/webpack.prod.config.js --env.ENVIRONMENT=staging --progress --env.ROLE=ua",
    "build-staging-client": "npm run clean && webpack --mode production --config webpack/webpack.prod.config.js --env.ENVIRONMENT=staging --progress --env.ROLE=uc",
    "build-production-admin": "npm run clean && webpack --mode production --config webpack/webpack.prod.config.js --env.ENVIRONMENT=production --progress --env.ROLE=ua",
    "build-production-client": "npm run clean && webpack --mode production --config webpack/webpack.prod.config.js --env.ENVIRONMENT=production --progress --env.ROLE=uc",
    "clean": "rm -rf dist/*",
    "lint:ts": "tslint --project tsconfig.json --config tslint.json",
    "lint:sass": "sass-lint --config .sass-lint.yml"
  },
